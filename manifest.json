{
    "name": "Extensions Guard",
    "version": "0.1.1.5",
    "description": "Extensions Guard",
    "manifest_version": 2,
    "minimum_chrome_version": "29",
    "permissions": [
    	"identity",
		"management",
        "storage",
        "alarms",
        "notifications"
	],
	"icons": {
	    "16": "assets/img/icons/icon16.png",
	    "48": "assets/img/icons/icon48.png",
	    "128": "assets/img/icons/icon128.png",
	    "256": "assets/img/icons/icon256.png"
  	},
    "key": "",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "browser_action": {
        "default_title": "Google Mail",
        "default_popup": "popup.html",
        "default_icon": {
            "16": "assets/img/icons/icon16.png",
            "48": "assets/img/icons/icon48.png",
            "128": "assets/img/icons/icon128.png",
            "256": "assets/img/icons/icon256.png"
        }
    },
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "oauth2": {
        "client_id": "1073387286098-gjhk6rafu7d2i5j10pauprg51ejqvevn.apps.googleusercontent.com",
        "scopes": [
          "https://www.googleapis.com/auth/userinfo.email"
        ]
    }
}
